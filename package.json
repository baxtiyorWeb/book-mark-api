{
  "name": "express-vercel-starter",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "scripts": {
   "build": "tsup app/index.ts --clean --format cjs --out-dir api",
    "dev": "tsx watch app/index.ts",
    "start": "tsx app/index.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "check": "biome check --apply .",
    "format": "biome format . --write",
    "lint": "biome lint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cookie-parser": "^1.4.7",
    "cors": "2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.1",
    "express": "5.1.0",
    "helmet": "8.1.0",
    "jsend": "1.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.5",
    "nodemailer": "^7.0.5",
    "pino": "9.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "useragent": "^2.3.0",
    "zod": "^4.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "^1.2.19",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jsend": "1.0.32",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/useragent": "^2.3.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "supertest": "7.1.4",
    "tsup": "8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  }
}
